<launch>
  <node pkg="ndt_omp" type="ndt_mapping_node" name="ndt_matching">
    <rosparam>
      resolution: 1.0
      step_size: 0.1
      trans_epsilon: 0.01
      max_iterations: 30
      leaf_size: 1.0
      use_imu: false
      debug: false
    </rosparam>
    <param name="input_topic" value="/velodyne_points"/>
    <param name="map_path" value="$(find me5413_localization)/map/all_downsampled_points.pcd"/>
    <param name="odom_frame" value="odom"/>
    <param name="base_frame" value="base_link"/>
  </node>
</launch>
